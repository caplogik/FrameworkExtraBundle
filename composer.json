{
    "name": "caplogik/framework-extra-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
	"require": {
        "php": "^7.3.31 || ^8.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Caplogik\\FrameworkExtraBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caplogik\\FrameworkExtraBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
